{
    "manifest_version": 2,
    "name": "Milico",
    "description": "Você pode substituir Intervenção Militar por Flores, por exemplo.",
    "version": "1.0",
    "icons": {
        "16": "16.png",
        "48": "48.png",
        "128": "128.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*/*"
            ],
            "js": [
                "content.js"
            ],
            "run_at": "document_end"
        }
    ]
}